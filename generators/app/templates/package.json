{
  "name": "<%= moduleName %>"
  ,"description": "<%= moduleDescription %>"
  ,"main": "index.js"
  ,"registry": "<%= moduleRegistry %>"
  ,"version": "0.0.0"<% if (private) { %>
  ,"private": true<% } %>
  ,"scripts": {
    "test": "mocha --harmony test/test.js"
    ,"postinstall": "jspm install"
    ,"lint": "beautilint index.js lib/*.js test/*.js"
  }
  ,"repository": {
    "type": "git"
    ,"url": "<%= gitUrl %><%= moduleName %>.git"
  }
  ,"author": "<%= author %>"
  ,"devDependencies": {
    "beautilint": "^0.1.0"
    ,"denodeify": "^1.2.1"
    ,"glob": "^7.0.3"
    ,"jspm": "^0.16.34"
    ,"mocha": "^2.4.5"
    ,"connect": "^3.4.1"
    ,"selenium-webdriver": "^2.53.2"
    ,"serve-static": "^1.10.3"
    ,"systemjs": "^0.19.29"
    ,"chai": "^3.5.0"
    ,"chai-as-promised": "^5.3.0"
    ,"sinon": "^1"
    ,"sinon-chai": "^2.8.0"
  }
  ,"jspm": {
    "configFile": "systemjs-config.js"<% if (angular) { %>
    ,"dependencies": {
      "angular": "github:angular/bower-angular@^1.5.5"
      ,"angular-mocks": "github:angular/bower-angular-mocks@^1.5.5"
      ,"angular-sanitize": "github:angular/bower-angular-sanitize@^1.5.5"
      ,"jquery": "npm:jquery@^2.2.3"
      ,"html": "github:systemjs/plugin-text@^0.0.7"
    }<% } %>
    ,"devDependencies": {
      "babel": "npm:babel-core@^5.8.24"
      ,"babel-runtime": "npm:babel-runtime@^5.8.24"
      ,"core-js": "npm:core-js@^1.1.4"
      ,"q": "github:kriskowal/q@^1"
    }<% if (angular) { %>
    ,"overrides": {
      "github:angular/bower-angular@1.5.5": {
        "shim": {
          "*": [
            "jquery"
          ]
        }
      }
    }<% } %>
  }
}
